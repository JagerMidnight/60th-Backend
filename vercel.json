{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/pages/home/index.html"
    },
    {
      "source": "/:path*/",
      "destination": "/pages/:path*/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/pages/:path*.html"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
