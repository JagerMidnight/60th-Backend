{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/messageCount.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/edit.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/register.html",
      "use": "@vercel/static"
    },
    {
      "src": "code/js/register.js",
      "use": "@vercel/static"  // Client-side JS (button presses, etc.)
    },
    {
      "src": "code/js/edit.js",
      "use": "@vercel/static"  // Client-side JS (button presses, etc.)
    },
    {
      "src": "code/css/register.css",
      "use": "@vercel/static"  // Client-side CSS
    },
    {
      "src": "code/css/edit.css",
      "use": "@vercel/static"  // Client-side CSS
    }
  ],
  "routes": [
    {
      "src": "/api/analytics/messageCount",
      "dest": "/api/analytics/messageCount.js"
    },
    {
      "src": "/edit",
      "dest": "/public/edit.html"
    },
    {
      "src": "/register",
      "dest": "/public/register.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    },
    {
      "src": "/register/script",
      "dest": "/code/js/register.js"  // Client-side JS (linked in HTML)
    },
    {
      "src": "/edit/script",
      "dest": "/code/js/edit.js"  // Client-side JS (linked in HTML)
    },
    {
      "src": "/register/css",
      "dest": "/code/css/register.css"  // Client-side CSS (linked in HTML)
    },
    {
      "src": "/edit/css",
      "dest": "/code/css/edit.css"  // Client-side CSS (linked in HTML)
    }
  ]
}
