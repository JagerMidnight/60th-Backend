{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/messageCount.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/messageCount",
      "dest": "/api/messageCount.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
